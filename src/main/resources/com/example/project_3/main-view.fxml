                              <RadioButton fx:id="MEMajor" layoutY="10.0" mnemonicParsing="false" text="ME">
                                 <padding>
                                    <Insets right="20.0" />
                                 </padding>
                              </RadioButton>
                              <RadioButton fx:id="ITMajor" layoutY="10.0" mnemonicParsing="false" text="IT">
                                 <padding>
                                    <Insets right="20.0" />
                                 </padding>
                              </RadioButton>
                              <RadioButton fx:id="BAMajor" layoutY="10.0" mnemonicParsing="false" text="BA">
                                 <padding>
                                    <Insets right="20.0" />
                                 </padding>
                              </RadioButton>
                           </children>
                        </HBox>
                        <Label layoutX="45.0" layoutY="109.0" text="Payment Amount  $" />
