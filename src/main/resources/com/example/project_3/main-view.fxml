                                       <children>
                                          <HBox prefHeight="100.0" prefWidth="143.0">
                                             <children>
                                                <RadioButton fx:id="residentRadioButton" mnemonicParsing="false" text="Resident">
                                                   <toggleGroup>
                                                      <ToggleGroup fx:id="residencyStatusSelectionGroup" />
                                                   </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="nonResidentRadioButton" mnemonicParsing="false" text="Non-Resident" toggleGroup="$residencyStatusSelectionGroup" />
                                             </children>
                                          </HBox>
                                          <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="143.0">
                                             <children>
                                                <RadioButton fx:id="tristateRadioButton1" disable="true" mnemonicParsing="false" text="Tristate">
                                                   <toggleGroup>
                                                      <ToggleGroup fx:id="nonResidencySelectionGroup" />
                                                   </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="connecticutRadioButton" disable="true" mnemonicParsing="false" text="Connecticut">
                                                   <toggleGroup>
                                                      <ToggleGroup fx:id="tristateSelectionGroup" />
                                                   </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="newYorkRadioButton11" disable="true" layoutX="70.0" layoutY="10.0" mnemonicParsing="false" text="New York" toggleGroup="$tristateSelectionGroup" />
                                             </children>
                                          </HBox>
                                          <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="143.0">
                                             <children>
                                                <RadioButton fx:id="internationalRadioButton" disable="true" mnemonicParsing="false" text="International" toggleGroup="$nonResidencySelectionGroup" />
